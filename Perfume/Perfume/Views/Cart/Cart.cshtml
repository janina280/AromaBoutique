@model List<CartModel>

@if (Model.Count() <= 0)
{
    <p>Nu s-au găsit parfumuri!</p>
}
<div class="container">
    <div class="title-cart">Coș de cumpărături</div>
</div>
<div class="row p-2 g-4">
    @foreach (var perfume in Model)
    {
        @await Html.PartialAsync("Components/CartCard", perfume)
    }
</div>
